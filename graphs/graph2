digraph finite_state_machine {
	rankdir=LR;
	size="8,5"
	node [shape = circle];
	0 -> 2 [ label = "0" ];
	0 -> 3 [ label = "0" ];
	0 -> 1 [ label = "0" ];
	2 -> 4 [ label = "4" ];
	3 -> 4 [ label = "5" ];
	3 -> 2 [ label = "2" ];
	3 -> 1 [ label = "1" ];
	1 -> 4 [ label = "1" ];
	1 -> 5 [ label = "4" ];
	4 -> 5 [ label = "6" ];
}
